generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Chamado {
  id          String   @id @default(uuid())
  ticket      String   @unique
  setor       String
  descricao   String
  status      String   @default("aberto")
  prioridade  String   @default("normal")
  anexoUrl    String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}